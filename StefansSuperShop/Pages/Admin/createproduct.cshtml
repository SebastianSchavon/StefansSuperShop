@page
@model StefansSuperShop.Pages.Admin.createproductModel
@{
}
<form method="post">
    <div class="mb-3 mt-3">
        <label class="form-label">Product Name </label>
        <input type="text" name="productName" class="form-control" placeholder="Product Name">
    </div>


    <div class="mb-3 mt-3">
        <label class="form-label">PUnit Price </label>
        <input type="text" name="unitPrice" class="form-control" placeholder="Unit Price">
    </div>

    <div class="mb-3 mt-3">
        <label class="form-label">Units InStock </label>
        <input type="text" name="unitsInStock" class="form-control" placeholder="Units InStock">
    </div>

    <div class="mb-3 mt-3">
        <label class="form-label">Rating </label>
        <input type="text" name="rating" class="form-control" placeholder="Rating">
    </div>
    <div class="mb-3 mt-3">
        <label class="form-label">CategoryID </label>
        <input type="text" name="categoryid" class="form-control" placeholder="CategoryID">
    </div>
    <div class="mb-3 mt-3">
        <label class="form-label">CampingPrice </label>
        <input type="text" name="campingprice" class="form-control" placeholder="CampingPrice">
    </div>
    <br />

    <button type="submit" class="btn btn-success">ADD</button>
    @*<a href="/Admin/Products" class=”btn btn-dark”>
        Product
        
         </a>*@

    <a class="btn btn-success" href="/Admin/Products" role="button">Product</a>
